{
    "name": "geekhives/package-qc-lgu",
    "description": "QC LGU Package",
    "require": {
        "php": "^7.3",
        "ext-json": "^1.7",
        "ext-curl": "^7.3",
        "illuminate/support": "^7.22"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3",
        "phpunit/phpunit": "^9.2"
    },
    "authors": [
        {
            "name": "Gerhard",
            "email": "ggdeocampo@multisyscorp.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
